{
  "name": "spesscomputer",
  "version": "1.0.0",
  "scripts": {
    "ext:build:web" : "cd godot-6502 && cargo +nightly build -Zbuild-std --target wasm32-unknown-emscripten",
    "ext:build" : "cd godot-6502 && cargo build && cargo +nightly build -Zbuild-std --target wasm32-unknown-emscripten",
    "godot:export" : "$GODOT4_BIN --headless --path godot --export-debug \"Web\" ../web/static/SpessComputer.html",
    "web:import" : "rm -f web/static/* && cp export/* web/static",
    "web:dev" : "cd web && npm run dev",
    "web:build" : "cd web && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nsarrazin/spesscomputer.git"
  },
  "keywords": [
    "6502"
  ],
  "author": "Nathan Sarrazin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nsarrazin/spesscomputer/issues"
  },
  "homepage": "https://github.com/nsarrazin/spesscomputer#readme"
}
