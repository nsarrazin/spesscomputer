OUTPUT = $0200
SPEED = $0204

.ORG $0600

LDY #$7F        ; Load Y with hex 127 (0x7F)

loop:
LDA SPEED       ; Load value from 0x0204
CPY $0204       ; Compare with 23 (Y register)
BCC write_0     ; If >= 23, branch to write_0
LDA #255        ; Load 255
JMP write_out   ; Skip over the 0 load

write_0:
LDA #0          ; Load 0

write_out:
STA OUTPUT      ; Store to 0x0200
JMP loop        ; Repeat